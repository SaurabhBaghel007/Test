#include <bits/stdc++.h>

using namespace std;

//problem statement 1
void solve(){
    int n,m;
    cin>>n>>m;
    int arr1[n],arr2[n];
    for(int i=0;i<n;i++) cin>>arr1[i];
    for(int i=0;i<m;i++) cin>>arr2[i];
    int _max = *max_element(arr1,arr1+n);
    for(int i=0;i<m;i++){
        if(_max==arr2[i]){
            cout<<"True"<<endl;
            return;
        }
    }
    cout<<"False"<<endl;
}

int main()
{
    int t;cin>>t;
    while(t--){
        solve();
    }

    return 0;
}
